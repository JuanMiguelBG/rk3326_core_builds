diff --git a/makefile.libretro b/makefile.libretro
index e0733b0..353421c 100644
--- a/makefile.libretro
+++ b/makefile.libretro
@@ -65,7 +65,7 @@ ifneq (, $(findstring clang,$(CXX)))
 endif
 
 # TARGET
-TARGET_NAME := fbalpha
+TARGET_NAME := fbalpha2018
 
 ifeq ($(PTR64), 1)
    FBA_DEFINES += -DPTR64
@@ -295,6 +295,25 @@ else ifeq ($(platform), classic_armv7_a7)
 	endif
 #######################################
 
+# Rockchip Rk3326 - 32-bit userspace
+
+else ifeq ($(platform), rk3326)
+   TARGET := $(TARGET_NAME)_libretro.so
+   fpic := -fPIC
+   SHARED := -shared -Wl,-no-undefined -Wl,--version-script=$(VERSION_SCRIPT)
+   ARCH = arm
+   HAVE_NEON = 1
+   BUILTIN_GPU = neon
+   USE_DYNAREC = 1
+   ENDIANNESS_DEFINES := -DLSB_FIRST
+   CFLAGS += -marm -mtune=cortex-a35 -mfpu=neon-fp-armv8 -mfloat-abi=hard -Ofast
+   CXXFLAGS += $(CFLAGS) -std=gnu++11
+   CFLAGS += -std=gnu11
+   ASFLAGS += -mcpu=cortex-a35 -mfpu=neon-fp-armv8 -mfloat-abi=hard
+   LDFLAGS += -fPIC -static-libgcc -static-libstdc++ -lpthread
+
+#######################################
+
 # Nintendo Game Cube / Wii / WiiU
 else ifneq (,$(filter $(platform), ngc wii wiiu))
    TARGET := $(TARGET_NAME)_libretro_$(platform).a
